    "when": "textInputFocus"
  },
  {
    "key": "alt+a",
    "command": "cursorLeft",
    "when": "textInputFocus"
  },
  {
    "key": "shift+alt+s",
    "command": "cursorDown",
    "when": "textInputFocus"
